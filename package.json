{
  "name": "lucid-dpt",
  "displayName": "Lucid DPT",
  "description": "VSCode support for the programmable switch language: Lucid DPT",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "lucid",
      "aliases": ["Lucid", "lucid", "LUCID"],
      "extensions": [".dpt"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "lucid",
      "scopeName": "source.lucid",
      "path": "./syntaxes/lucid.tmLanguage.json"
    }]
  }
}
